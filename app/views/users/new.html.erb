<h2>New Owner</h2>
<%= form_tag new_user_path(@user) do %>
    Username: <%= text_field_tag :username %><br>
    <%= render 'form' %>
    Password: <%= password_field_tag :password %><br>
    Password Confirmation: <%= password_field_tag :password_confirmation %><br>
    <%= submit_tag "Create New Owner" %>
<% end %>