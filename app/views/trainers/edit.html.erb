<h2><%= @trainer.name %>'s Info</h2>
<%= form_with(model: @trainer) do |f| %>
    Username: <%= f.text_field :username %><br>
    First Name: <%= f.text_field :first_name %><br>
    Last Name: <%= f.text_field :last_name %><br>
    Phone Number: <%= f.telephone_field :phone_number %><br>
    Email: <%= f.text_field :email %><br>
    Credentails: <%= f.text_field :certification %><br>
    <%= f.submit %>
<% end %>