<h3>Owner</h3>
<%= form_with do |f| %>
    <%= f.label "Username"%>
    <%= f.text_field "user[username]" %><br>
    <%= f.label "Password"%>
    <%= f.password_field "user[password]" %><br>
    <%= f.submit "Log In Owner"%>
<% end %>

<h3>Trainer</h3>
<%= form_with do |f| %>
    <%= f.label "Username"%>
    <%= f.text_field "trainer[username]" %><br>
    <%= f.label "Password"%>
    <%= f.password_field "trainer[password]" %><br>
    <%= f.submit "Log In Trainer"%>
<% end %>
